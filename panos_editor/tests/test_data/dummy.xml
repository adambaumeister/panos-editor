<?xml version="1.0"?>
<config>
    <shared>
        <address>
            <entry name="testhost_10.100.100.10">
                <ip-netmask>10.100.100.10</ip-netmask>
                <description>The Ubuntu test host.</description>
                <tag>
                    <member>DEMO-STATIC</member>
                    <member>SECOND-TAG</member>
                </tag>
            </entry>
            <entry name="testhost_public_ip_DYNAMIC">
                <ip-netmask>20.213.243.31</ip-netmask>
                <description>Dynamic address of public test host, will change per deployment.</description>
                <tag>
                    <member>DEMO-DYNAMIC</member>
                    <member>SECOND-TAG</member>
                </tag>
            </entry>
        </address>
    </shared>
    <device>
        <entry name="localhost.localdomain">
            <device-group>
                <entry name="INBOUND">
                    <post-rulebase>
                        <security>
                            <rules>
                                <entry name="OUTBOUND" uuid="24fcda30-a252-4025-ae63-d1e0965ced5d">
                                    <from>
                                        <member>PRIVATE</member>
                                    </from>
                                    <to>
                                        <member>PUBLIC</member>
                                    </to>
                                    <source>
                                        <member>testhost_10.100.100.10</member>
                                    </source>
                                    <source-user>
                                        <member>any</member>
                                    </source-user>
                                    <hip-profiles>
                                        <member>any</member>
                                    </hip-profiles>
                                    <destination>
                                        <member>any</member>
                                    </destination>
                                    <application>
                                        <member>any</member>
                                    </application>
                                    <service>
                                        <member>application-default</member>
                                    </service>
                                    <category>
                                        <member>any</member>
                                    </category>
                                    <action>allow</action>
                                    <log-setting>forward_panorama</log-setting>
                                    <log-end>yes</log-end>
                                    <description>Allow all outbound traffic.</description>
                                    <target>
                                        <negate>no</negate>
                                    </target>
                                </entry>
                            </rules>
                        </security>
                    </post-rulebase>
                </entry>
            </device-group>
        </entry>
    </device>
</config>
